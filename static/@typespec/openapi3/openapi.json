  "components": {
    "schemas": {
      "ApiError": {
        "type": "object",
        "required": [
          "status",
          "code",
          "message"
        ],
        "properties": {
          "status": {
            "type": "number",
            "enum": [
              500,
              504,
              400,
              401,
              403,
              404,
              405
            ]
          },
          "code": {
            "type": "string",
            "enum": [
              "bad_database_response",
              "bad_header",
              "missing_required_header",
              "bad_query_input",
              "database_timeout",
              "forbidden",
              "internal_server_error",
              "method_not_allowed",
              "route_not_found",
              "unauthorized"
            ]
          },
          "message": {
            "type": "string"
          }
        }
      },
      "Models.Actions": {
        "type": "object",
        "required": [
          "block_time",
          "block_number",
          "block_hash",
          "block_date",
          "tx_hash",
          "tx_index",
          "tx_status",
          "tx_status_code",
          "tx_success",
          "abi_sequence",
          "code_sequence",
          "digest",
          "global_sequence",
          "receipt_receiver",
          "recv_sequence",
          "account",
